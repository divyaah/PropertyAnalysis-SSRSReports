    <DataSet Name="SelectCity">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DISTINCT DimGeography.City
FROM         DimTransport LEFT OUTER JOIN
                         DimGeography ON DimTransport.Suburb = DimGeography.Suburb
WHERE     (DimGeography.City IS NOT NULL)
ORDER BY DimGeography.City</CommandText>
      </Query>
      <Fields>
        <Field Name="City">
          <DataField>City</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="SelectSuburb">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CitySelect">
            <Value>=Parameters!CitySelect.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT DimTransport.Suburb
FROM         DimTransport LEFT OUTER JOIN
                         DimGeography ON DimTransport.DimGeographyKey = DimGeography.DimGeographyKey
WHERE     (DimGeography.City = @CitySelect)
ORDER BY  DimTransport.Suburb ASC</CommandText>
      </Query>
      <Fields>
        <Field Name="Suburb">
